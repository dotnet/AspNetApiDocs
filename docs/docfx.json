{
  "metadata": [{
    "src": [{
      "files": [
        "**/src/**/project.json"
      ],
      "exclude": [
        "../aspnet/razor/src/Microsoft.AspNet.Razor.Runtime.VSRC1/*",
        "../aspnet/razor/src/Microsoft.AspNet.Razor.VSRC1/*",
        "../aspnet/mvc/src/Microsoft.AspNet.Mvc.Razor.Host.VSRC1/*",
        "../aspnet/filesystem/src/Microsoft.AspNet.FileProviders.Abstractions.VSRC1/*",
        "../aspnet/filesystem/src/Microsoft.AspNet.FileProviders.Physical.VSRC1/*",
        "../aspnet/caching/src/Microsoft.Extensions.Caching.Abstractions.VSRC1/*",
        "../aspnet/caching/src/Microsoft.Extensions.Caching.Memory.VSRC1/*",
        "../aspnet/options/src/Microsoft.Extensions.OptionsModel.VSRC1/*"
      ],
      "src": "../aspnet"
    }],
    "dest": "api/",
    "shouldSkipMarkup": true
  }],
  "build": {
    "content": [
      {
        "files": [
          "**/*.md"
        ],
        "exclude": [
          "api/**"
        ],
        "dest": "."
      },
      {
        "files": [
          "**/*.yml",
          "**/*.md"
        ],
        "src": "api",
        "dest": "."
      }
    ],
    "resource": [
      {
        "files": [
          "**/*.png",
          "**/*.jpg",
          "**/*.gif",
          "breadcrumb.json"
        ],
        "exclude": [
          "**/obj/**",
          "aspnet/api/**",
          "**/includes/**"
        ]
      }
    ],
    "overwrite": [],
    "externalReference": [],
    "globalMetadata": {
      "breadcrumb_path": "/aspnet/core/api/breadcrumb.json",
      "ms.locale": "en-us",
      "ROBOTS": "INDEX,FOLLOW",
      "author": "rick-anderson",
      "ms.author": "riande",
      "manager": "wpickett",
      "ms.date": "09/15/2016",
      "ms.topic": "reference",
      "audience": "developer",
      "ms.reviewer": "daroth",
      "ms.prod": "aspnet-core",
      "ms.technology": "aspnet",
      "ms.tgt_pltfrm": "dotnet",
      "searchScope": [".NET", ".NET Core", "ASP.NET Core"] 
    },
    "fileMetadata": {},
    "template": [],
    "dest": "_site"
  }
}
