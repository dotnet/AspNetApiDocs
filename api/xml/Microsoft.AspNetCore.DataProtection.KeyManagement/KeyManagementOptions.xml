      <MemberSignature Language="ILAsm" Value=".property instance bool AutoGenerateKeys" />
      <MemberSignature Language="DocId" Value="P:Microsoft.AspNetCore.DataProtection.KeyManagement.KeyManagementOptions.AutoGenerateKeys" />
